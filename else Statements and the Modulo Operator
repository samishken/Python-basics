def fraction_part(numerat, denominat):
  if denominat != 0:
    return (numerat % denominat) / denominat
  else:
    return 0

 
print(fraction_part(5, 5)) # Should be 0
print(fraction_part(5, 4)) # Should be 0.25
print(fraction_part(5, 3)) # Should be 0.66...
print(fraction_part(5, 2)) # Should be 0.5
print(fraction_part(5, 0)) # Should be 0
print(fraction_part(0, 5)) # Should be 0

